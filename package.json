{
  "name": "dorsale",
  "type": "module",
  "version": "0.1.25",
  "main": "./src/index.ts",
  "description": "Dorsale is a Bun-based TypeScript backend framework",
  "scripts": {
    "debug": "bun --watch test/debug.ts",
    "test": "bun test",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "check-format": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint src test",
    "check-types": "tsc --noEmit"
  },
  "files": [
    "src",
    "package.json",
    "tsconfig.json",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dorsale/dorsale.git"
  },
  "keywords": [
    "bun",
    "typescript",
    "backend",
    "framework",
    "dependency injection",
    "inversion of control",
    "api",
    "rest",
    "lightweight",
    "fast",
    "performance"
  ],
  "author": "Joel DIBASSO <joel@dibasso.fr>",
  "license": "MIT",
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.12.0",
    "@types/bun": "latest",
    "@types/node": "^24.0.2",
    "@typescript-eslint/eslint-plugin": "^8.9.0",
    "@typescript-eslint/parser": "^8.10.0",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^10.0.1",
    "globals": "^16.0.0",
    "prettier": "3.6.2"
  },
  "dependencies": {
    "@dorsale/commons": "^1.0.0",
    "@typescript-eslint/typescript-estree": "^8.9.0",
    "acorn": "^8.13.0",
    "estree-walker": "^3.0.3",
    "fast-querystring": "^1.1.2",
    "reflect-metadata": "^0.2.1",
    "wint-js": "^1.2.7"
  }
}
